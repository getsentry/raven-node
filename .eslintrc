{
  "env": {
    "node": true,
    "mocha": true,
    "es6": true
  },
  "rules": {
    "block-scoped-var": 2,
    "camelcase": [1, "properties": "never"],
    "comma-dangle": 0,
    "consistent-return": 2,
    "consistent-this": [2, "self"],
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": [2, "allow-null"],
    "guard-for-in": 2,
    "new-cap": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-catch-shadow": 1,
    "no-console": 1,
    "no-debugger": 2,
    "no-else-return": 2,
    "no-empty": [2, {"allowEmptyCatch": true}],
    "no-eval": 2,
    "no-extra-bind": 2,
    "no-extend-native": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-inner-declarations": 2,
    "no-iterator": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-new": 2,
    "no-param-reassign": 1,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-shadow-restricted-names": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-underscore-dangle": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-use-before-define": [2, "nofunc"],
    "no-useless-call": 2,
    "no-useless-escape": 2,
    "no-warning-comments": 0,
    "no-with": 2,
    "spaced-comment": [2, "always"],
    "strict": [2, "global"],
    "valid-jsdoc": 0,
    "valid-typeof": 2,
    "yoda": [2, "never"]
  },
  "extends": [
    "prettier"
  ]
}
